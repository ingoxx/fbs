@charset "UTF-8";
.box {
  display: flex;
  flex-direction: column;
  height: 100vh;
  background: #6a72d9;
}

.box .location {
  display: flex;
  justify-content: center;
  /* 水平居中 */
  align-items: center;
  /* 垂直居中 */
  height: 180rpx;
  background: #7880dd;
  padding: 0 20rpx;
}

.box .location .coordinate {
  color: #ffffff;
  font-size: 25rpx;
  line-height: 1.4;
  word-break: break-word;
  white-space: normal;
  font-weight: 600;
}

.box .search {
  margin: 0 25rpx;
  margin-top: 30rpx;
  margin-bottom: 17rpx;
  background: #ffffff;
  border-radius: 16rpx;
}

.box .search .se {
  padding: 0 0;
}

.box .search .s-p {
  width: 80%;
  margin: 19rpx auto;
  padding-top: 20rpx;
}

.box .search .s-p .s-p-1 {
  height: 70rpx;
  border: 1rpx #d2d2d2 solid;
  border-radius: 9rpx;
  font-size: 25rpx;
  padding-left: 21rpx;
  padding-right: 60rpx;
}

.box .search .s-b {
  width: 80%;
  margin: 16rpx auto;
  padding-bottom: 10rpx;
}

.box .card-1 {
  display: flex;
  justify-content: center;
  justify-items: center;
  flex-wrap: wrap;
  overflow-y: auto;
  box-sizing: border-box;
}

.box .card-1 .card-1-1 {
  width: 310rpx;
  margin: 14rpx 14rpx;
  background: #ffff;
  border-radius: 10rpx;
  display: inline-grid;
  padding: 10rpx;
}

.box .card-1 .card-1-1 .online-status {
  position: relative;
  bottom: 14rpx;
  width: 20rpx;
  height: 20rpx;
}

.box .card-1 .card-1-1 .img, .box .card-1 .card-1-1 .addr, .box .card-1 .card-1-1 .distance, .box .card-1 .card-1-1 .tags, .box .card-1 .card-1-1 .online {
  display: flex;
  justify-content: center;
}

.box .card-1 .card-1-1 .addr .t-f {
  font-size: 28rpx;
  font-weight: 700;
  color: #3d3d3d;
}

.box .card-1 .card-1-1 .distance .t-f {
  font-size: 20rpx;
  color: #3d3d3d;
}

.box .card-1 .card-1-1 .tags {
  gap: 20rpx;
}

.box .basket {
  color: #ffffff;
}

.box .contact-us {
  position: fixed;
  top: 56%;
}

.box .contact-us .share-btn {
  opacity: 0.7;
  width: 93rpx;
  color: #2d2c2c;
  font-size: 37rpx;
}

.box .flush-data {
  position: fixed;
  top: 56%;
  right: 0;
}

.box .flush-data .share-btn {
  opacity: 0.7;
  width: 93rpx;
  color: #2d2c2c;
  font-size: 37rpx;
}

.van-c {
  padding: 10rpx 0;
}

.van-b-isactive {
  background-color: #715dbc !important;
  color: #ffffff !important;
}

.van-tag-c {
  background: #909090 !important;
}

.c-van-tag-online-status {
  width: 20rpx !important;
  height: 20rpx !important;
  background: #ffffff;
}

.custom-selector-load {
  width: auto !important;
}

.van-transition-c {
  float: right !important;
  position: relative !important;
  bottom: 58rpx !important;
  right: 19rpx !important;
}

.check-list-box {
  display: flex;
  flex-direction: column;
  justify-content: center;
  height: 250rpx;
  overflow-y: auto;
  -webkit-backface-visibility: hidden;
  -webkit-transform: translateZ(0);
}

.check-list-data {
  width: 70%;
  display: flex;
  margin: 0 auto;
  border-bottom: 1rpx solid #d2d2d2;
}

.check-list-data .check-id, .check-list-data .check-addr, .check-list-data .check-submit {
  padding: 10rpx 10rpx;
}

.check-list-data .submit-flex {
  display: flex;
  justify-content: center;
  gap: 53rpx;
}

.check-list-data .addr-size {
  font-size: 26rpx;
}

.tx-map {
  width: 93%;
  margin: 0 auto;
}

.van-empty__description {
  color: #dcdcdc !important;
}

.van-dialog-c .van-dialog__footer {
  text-align: center !important;
}

.privacy-title {
  text-align: center;
  margin: 18rpx 0;
  font-weight: 700;
}

.privacy-center .privacy-content {
  padding: 0 11rpx;
}

.privacy-center .privacy-content .privacy-item {
  font-weight: 600;
  font-size: 28rpx;
}

.privacy-center .privacy-content .privacy-detail {
  display: block;
  padding: 10rpx 23rpx;
  font-size: 28rpx;
}

.privacy-center .privacy-status {
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 17rpx 0;
  gap: 68rpx;
}

.van-icon-l {
  position: relative;
  bottom: 3rpx;
  color: #ff6161;
}

.sports-list-box {
  display: flex;
  flex-direction: inherit;
  justify-content: center;
  align-items: center;
  gap: 37rpx;
  flex-wrap: wrap;
  padding: 20rpx 20rpx;
}
